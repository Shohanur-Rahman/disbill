
@{
    ViewBag.Title = "registerresult";
    Layout = "~/Views/Shared/_BeforLoginLayout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        @if (string.IsNullOrEmpty(ViewBag.SuccessMessage) == false)
        {
            <h6>@ViewBag.SuccessMessage</h6>
        }
        else if (string.IsNullOrEmpty(ViewBag.errorMessage) == false)
        {
            <h6>@ViewBag.errorMessage</h6>
        }

    </div>
</div>
