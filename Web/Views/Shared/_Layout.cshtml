<body id="day-mode">

    <header class="edgpay-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="float-left">
                        <a class="nav-link active" href="~/"><img src="@Url.Content("~/images/logo.png?v=" + SiteSettings.UploadVersion)" alt="DG Pay Logo" class="login-top-logo" /></a>
                    </div>
                    <div class="float-right">
                        <ul class="nav login-nav-right">
                            <li class="nav-item">
                                <a class="nav-link" href="~/profile/change-pssword">পাসওয়ার্ড পরিবর্তন</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link active" href="~/profile">@LoggedInUserInfoFromCookie.UserFirstNameInCookie</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </header>

    <section class="edgpay-page-body">
        <div class="container">
            @RenderBody()
        </div>
    </section>

    <footer class="edgpay-footer">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <ul class="edgpay-footer-menu">
                        <li class="edgpayd-footer-menu-item">
                            <a href="~/">
                                <i class="edgpayd-item-icon fas fa-house-damage"></i>
                                <span class="edgpayd-item-name">হোম</span>
                            </a>
                        </li>
                        <li class="edgpayd-footer-menu-item">
                            <a href="~/profile">
                                <i class="edgpayd-item-icon far fa-id-badge"></i>
                                <span class="edgpayd-item-name">প্রোফাইল</span>
                            </a>
                        </li>
                        <li class="edgpayd-footer-menu-item">
                            <a href="social.html">
                                <i class="edgpayd-item-icon fas fa-question"></i>
                                <span class="edgpayd-item-name">সাহায্য</span>
                            </a>
                        </li>
                        <li class="edgpayd-footer-menu-item">
                            <a href="@Url.Action("Logout", "account")">
                                <i class="edgpayd-item-icon fas fa-power-off"></i>
                                <span class="edgpayd-item-name">লগআউট</span>
                            </a>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
    </footer>


